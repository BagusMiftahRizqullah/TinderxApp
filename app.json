{
  "expo": {
    "name": "Matchverse",
    "slug": "matchverse",
    "scheme": "matchverse",
    "sdkVersion": "52.0.0",
    "icon": "./src/components/icons/icon.png",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.miftahchaos.matchverse"
    },
    "android": {
      "package": "com.miftahchaos.matchverse"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}